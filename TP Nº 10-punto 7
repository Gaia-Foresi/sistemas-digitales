<!DOCTYPE html>
<html>
<head>
    <title>Claves de Usuarios</title>
</head>
<body>
    <?php
    $usuarios = [
        "usuario1" => "clave1",
        "usuario2" => "clave2",
        "usuario3" => "clave3",
        "usuario4" => "clave4",
        "usuario5" => "clave5"
    ];
    foreach ($usuarios as $nombre => $clave) {
        echo "<p>Usuario: $nombre, Clave: $clave</p>";
    }
    ?>
</body>
</html>
